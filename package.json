{
  "name": "portfolio",
  "version": "1.7.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true npm run build",
    "check": "biome check .",
    "check:write": "biome check --write .",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "type-check": "tsc --noEmit",
    "verify": "npm run type-check && npm run check",
    "cleanup": "pwsh scripts/cleanup.ps1",
    "cleanup:bash": "bash scripts/cleanup.sh",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "e2e": "playwright test",
